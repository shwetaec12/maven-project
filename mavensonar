pipeline
{
agent any{
 stage {
	stage ('clone my code')
		{
			steps {
				git branch:master'https://github.com/shwetaec12/maven-project'}
				}
		}
	stage {

		withSonarQubeEnv(credentialsId: 'c08fe7a6f20f613622ea7fb372bc887608d2c57b')
			{
				withMaven(jdk: 'localjdk-1.8', maven: 'localmaven')
					{
						sh'clean mvn install sonar:sonar'
					}
			}
		}
}}}
