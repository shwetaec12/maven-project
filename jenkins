pipeline
{
	agent any
		stages
		{
		stage('SCM checkout')
			{
			steps
      				{
				git branch: 'master', url: 'https://github.com/shwetaec12/maven-project'
			        }
			}
	stage ('Parallel Stage'){
	parallel {
	{
		stage(Compile Stage'){

		steps{
			withMaven(maven: 'localmaven')
					
			sh'mvn compile'
					
			}
		}
		stage(Test Stage'){

		steps{
			withMaven(maven: 'localmaven')
					
			sh'mvn Test'
					
			}
		}
		stage(Package Stage'){

		steps{
			withMaven(maven: 'localmaven')
					
			sh'mvn package'
					
			}
		}

}}}}
